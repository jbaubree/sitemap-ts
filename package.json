{
  "name": "sitemap-ts",
  "version": "0.1.3",
  "packageManager": "pnpm@6.32.11",
  "description": "Sitemap generator for TypeScript projects",
  "keywords": [
    "Sitemap",
    "Typescript",
    "Generator",
    "Robots",
    "Sitemap.xml",
    "Robots.txt"
  ],
  "homepage": "https://github.com/jbaubree/sitemap-ts#readme",
  "bugs": {
    "url": "https://github.com/antfu/sitemap-ts/issues"
  },
  "license": "MIT",
  "author": "JB Aubr√©e <jb.aubree@protonmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbaubree/sitemap-ts.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.22.0",
    "@antfu/ni": "0.14.0",
    "@antfu/utils": "0.5.1",
    "@babel/types": "7.17.10",
    "@types/node": "17.0.30",
    "bumpp": "7.1.1",
    "c8": "7.11.2",
    "eslint": "8.14.0",
    "esno": "0.14.1",
    "fast-glob": "3.2.11",
    "pnpm": "6.32.11",
    "rimraf": "3.0.2",
    "sitemap": "7.1.1",
    "typescript": "4.6.4",
    "unbuild": "0.7.4",
    "vite": "2.9.6",
    "vitest": "0.10.0",
    "xml-formatter": "2.6.1"
  }
}
